@page "/"
@namespace Demo.ServerSideApp.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Demo.ServerSideApp</title>
    <base href="~/" />
    <link rel="stylesheet" href="css/bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="css/site.css" />
    <link rel="stylesheet" href="highlight/default.css" />
    <link rel="stylesheet" href="_content/BlazorDateRangePicker/daterangepicker.min.css" />
</head>
<body>
    <app>@(await Html.RenderComponentAsync<App>(RenderMode.Server))</app>

    <script src="_framework/blazor.server.js"></script>
    <script src="highlight/highlight.pack.js"></script>
    <script src="highlight/highlightInterop.js"></script>
    <script src="_content/BlazorDateRangePicker/clickAndPositionHandler.js"></script>
</body>
</html>
